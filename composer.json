{
    "name": "laminas-friends/laminasfriends-mvc-user",
    "description": "A generic user registration and authentication module for Laminas. Supports Laminas\\Db and Doctrine2.",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "authentication"
    ],
    "homepage": "https://github.com/Laminas-Friends/LFUser",
    "authors": [
        {
            "name": "Alex Zamponi",
            "email": "no@spam.com",
            "homepage": "http://wwww.spam.com/"
        }
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-authentication": "^2.7",
        "laminas/laminas-crypt": "^3.3",
        "laminas/laminas-form": "^2.14",
        "laminas/laminas-inputfilter": "^2.10",
        "laminas/laminas-loader": "^2.6",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-validator": "^2.13",
        "laminas/laminas-db": "^2.11",
        "laminas/laminas-view": "^2.11",
        "laminas/laminas-session": "^2.9",
        "laminas/laminas-http": "^2.12",
        "laminas/laminas-mvc-plugin-flashmessenger": "^1.2",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-mvc-plugin-prg": "^1.2",
        "laminas/laminas-hydrator": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "3.*",
        "laminas/laminas-captcha": "^2.9",
        "laminas/laminas-text": "^2.7.1"
    },
    "suggest": {
        "laminas/laminas-captcha": "Laminas\\Captcha if you want to use the captcha component"
    },
    "autoload": {
        "psr-4": {
            "LaminasFriends\\Mvc\\User\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasFriendsTest\\Mvc\\User\\": "test/"
        }
    }
}
